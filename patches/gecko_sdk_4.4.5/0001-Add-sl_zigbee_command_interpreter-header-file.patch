+                                                      uint8_t *length);
+
+// TODO: doxygen
+uint8_t sl_zigbee_copy_string_arg(sl_cli_command_arg_t *arguments,
+                                  int8_t index,
+                                  uint8_t *destination,
+                                  uint8_t max_length,
+                                  bool left_pad);
